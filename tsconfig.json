{
    "compilerOptions": {
        "target": "ES5",
        "module": "commonjs",
        "noImplicitAny": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": false,
        "noResolve": true
    },
    "filesGlob": [
        "./*.ts",
		"./public/**/*.ts",
        "./models/**/*.ts",
        "./services/**/*.ts",
        "./typings/**/*.d.ts",
        "./node_modules/phaser/typescript/phaser"
    ]
}
